    "@vue/eslint-config-standard": "^5.1.2",
    "eslint": "^7.20.0",
    "eslint-config-vuepress": "^3.1.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^7.6.0",
    "typescript": "^4.1.5",
    "vitepress": "^0.11.5"

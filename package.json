{
  "name": "windicss",
  "version": "2.0.1-beta",
  "description": "windicss is a css compiler or css interpreter, which is based on the grammar of tailwindcss and adds other features.",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "windicss": "cli/index.js"
  },
  "scripts": {
    "dev": "tsc --build src --watch",
    "test": "tsc -p ./test --watch",
    "build": "rm -rf ./dist && tsc --build src && copyfiles * ./dist && rm ./dist/tsconfig.tsbuildinfo",
    "example": "tsc -p ./example --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voorjaar/windicss.git"
  },
  "keywords": [
    "tailwindcss",
    "css",
    "compiler"
  ],
  "author": "Veritas Raven",
  "license": "MIT",
  "homepage": "https://github.com/voorjaar/windicss",
  "dependencies": {
    "arg": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "copyfiles": "^2.4.1",
    "typescript": "^4.1.3"
  }
}
